<div class="w3-margin-left w3-row-padding" >
<h3>Wyszukaj</h3>
</div>
<form class="w3-margin-left w3-row-padding" id="form">
	<p>
	<label>Wyszukiwarka</label>
	<input onkeyup="showUser(this.value)" class="w3-input w3-border search" id="ST" type="text" style="width:20%"></p>
	<p>
	<label>Nazwa</label>
	<input onkeyup="showUser(this.value)" class="w3-input w3-border search" id="Nazwa" type="text" style="width:20%"></p>
	<p>
	<label>Nr seryjny</label>
	<input class="w3-input w3-border search" id="Nr_seryjny" type="text" style="width:20%"></p>
	<p>
	<label>Osoba pożyczająca</label>
	<input class="w3-input w3-border" name="Osoba_poz" type="text" style="width:20%"></p>
	<p>
	<label>Wydano</label>
	<input class="w3-input w3-border" name="Wydano" type="text" style="width:20%"></p>
	<p>
	<label>Do zwrotu</label>
	<input class="w3-input w3-border" name="Do_zwrotu" type="text" style="width:20%"></p>
	<p>
	<label>Konserwacja</label>
	<input class="w3-input w3-border" name="Konserwacja" type="text" style="width:20%"></p>
	
	<button class="w3-button w3-white w3-border" id="btnSubmit" type="submit">Szukaj</button>
</form>

<div id="wyniki"></div>

<script>


function showUser(str) {
  if (str == "") {
    document.getElementById("wyniki").innerHTML = "";
    return;
  } else {
    var xmlhttp = new XMLHttpRequest();
    xmlhttp.onreadystatechange = function() {
      if (this.readyState == 4 && this.status == 200) {
        document.getElementById("wyniki").innerHTML = this.responseText;
      }
    };
    xmlhttp.open("GET","getst.php?q="+str,true);
    xmlhttp.send();
  }
}
</script>