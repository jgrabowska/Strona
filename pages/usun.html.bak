<div class="w3-margin-left w3-row-padding" >
<h3>Usuń</h3>
</div>
<form class="w3-margin-left w3-row-padding" >
	<p>
	<label>ŚT</label>
	<input onkeyup="showdST(this.value)" class="w3-input w3-border search" id="ST" type="text" style="width:20%"></p>

	<button class="w3-button w3-white w3-border" onclick="dST()">Usuń</button>
</div>

<div id="wyniki"></div>

<script>
function showdST(str) {
  if (str == "") {
    document.getElementById("wyniki").innerHTML = "";
    return;
  } else {
    var xmlhttp = new XMLHttpRequest();
    xmlhttp.onreadystatechange = function() {
      if (this.readyState == 4 && this.status == 200) {
        document.getElementById("wyniki").innerHTML = this.responseText;
      }
    };
    xmlhttp.open("GET","delete.php?q="+str,true);
    xmlhttp.send();
  }
}
function dST(str) {
  if (str == "") {
    document.getElementById("wyniki").innerHTML = "";
    return;
  } else {
    var xmlhttp = new XMLHttpRequest();
    xmlhttp.onreadystatechange = function() {
      if (this.readyState == 4 && this.status == 200) {
        document.getElementById("wyniki").innerHTML = this.responseText;
      }
    };
    xmlhttp.open("GET","delst.php?q="+str,true);
    xmlhttp.send();
  }
}
</script>