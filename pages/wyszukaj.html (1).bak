<div class="w3-margin-left">
<h3>Wyszukaj</h3>
<input onkeyup="showST(this.value)" class="w3-input w3-border search" id="ST" type="text" style="width:20%">
<br><br>
<div id="wyszuk"></div>

<script>
function showST(str) {
  if (str == "") {
    document.getElementById("wyszuk").innerHTML = "";
    return;
  } else {
    var xmlhttp = new XMLHttpRequest();
    xmlhttp.onreadystatechange = function() {
      if (this.readyState == 4 && this.status == 200) {
        document.getElementById("wyszuk").innerHTML = this.responseText;
      }
    };
    xmlhttp.open("GET","getst.php?q="+str,true);
    xmlhttp.send();
  }
}
</script>
</div>